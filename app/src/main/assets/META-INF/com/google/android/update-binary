#!/sbin/sh
ZIP=$3
OUTFD=$2
SDAT=/data/system

ui_print() {
  echo -n -e "ui_print $1\n" > /proc/self/fd/$OUTFD
  echo -n -e "ui_print\n" > /proc/self/fd/$OUTFD
}

ui_print "*********************"
ui_print "-  ATMS Restorer   -"
ui_print "*********************"
